{
  "name": "bajarent",
  "version": "0.7.11",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "npx expo export:web",
    "type-check": "tsc --noEmit ",
    "lint:biome": "biome check --files-ignore-unknown=true .",
    "format:fix": "tsc --noEmit && biome check . && biome format --write . ",
    "format:biome": "biome format --write --files-ignore-unknown=true .",
    "merge:main": "git switch main && git merge dev && git switch dev",
    "push:main": "git push origin main",
    "fast-test": " tsc --noEmit && bun test",
    "fast-push": "npm run fast-test && npm run merge:main && npm run push:main",
    "fast-push:patch": "npm version patch && npm run merge:main && npm run push:main",
    "cy:open": "cypress open --e2e --browser chrome",
    "cy:run": "cypress run --e2e --browser chrome",
    "push:patch": "npm run fast-test && npm version patch --follow-tags && git push",
    "push:minor": "npm run fast-test && npm version minor --follow-tags && git push",
    "commit": "git status && bun type-check  && bun cy:run && git add . &&  git commit"
  },
  "dependencies": {
    "@expo/webpack-config": "^19.0.0",
    "@react-native-async-storage/async-storage": "1.18.2",
